  <!-- Class: DQ_IndirectEvaluation -->
  <!--++++++++++++++++++++++++++++++-->
  <owl:Class rdf:about="&iso19157;DQ_IndirectEvaluation">
    <rdfs:label>DQ_IndirectEvaluation</rdfs:label>
