    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19157;DQ_SampleBasedInspection.lotDescription"/>
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19157;DQ_SampleBasedInspection.lotDescription"/>
        <owl:allValuesFrom rdf:resource="&xsd;string"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19157;DQ_SampleBasedInspection.samplingRatio"/>
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19157;DQ_SampleBasedInspection.samplingRatio"/>
        <owl:allValuesFrom rdf:resource="&xsd;string"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

