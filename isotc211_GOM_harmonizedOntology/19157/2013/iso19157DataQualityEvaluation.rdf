  <!--++++++++++++++++++++++++++++++++++++++++++++++++++++++++++-->
  <owl:ObjectProperty rdf:about="&iso19157;DQ_EvaluationMethod.evaluationMethodType">
    <rdfs:label>evaluationMethodType</rdfs:label>
