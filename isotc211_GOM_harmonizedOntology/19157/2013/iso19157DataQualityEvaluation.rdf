      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19157;DQ_SampleBasedInspection.samplingScheme"/>
        <owl:allValuesFrom rdf:resource="&xsd;string"/>
