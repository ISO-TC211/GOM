<!DOCTYPE rdf:RDF [
  <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
  <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
  <!ENTITY dct "http://purl.org/dc/terms/" >
  <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
  <!ENTITY iso19150-2 "http://def.isotc211.org/iso19150/-2/2012/base#" >
  <!ENTITY iso19115_613 "http://def.isotc211.org/iso19115/2003/CitationAndResponsiblePartyInformation#" >
  <!ENTITY iso19115 "http://def.isotc211.org/iso19115/2003/PortrayalCatalogueInformation#" >

]>

<rdf:RDF xmlns="http://def.isotc211.org/iso19115/2003/PortrayalCatalogueInformation#"
  xml:base="http://def.isotc211.org/iso19115/2003/PortrayalCatalogueInformation"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:dct="http://purl.org/dc/terms/"
  xmlns:skos="http://www.w3.org/2004/02/skos/core#"
  xmlns:iso19150-2="http://def.isotc211.org/iso19150/-2/2012/base#"
  xmlns:iso19115_613="http://def.isotc211.org/iso19115/2003/CitationAndResponsiblePartyInformation#"
  xmlns:iso19115="http://def.isotc211.org/iso19115/2003/PortrayalCatalogueInformation#">
  <owl:Ontology rdf:about="http://def.isotc211.org/iso19115/2003/PortrayalCatalogueInformation">
    <rdfs:label>ISO 19115:2003 Portrayal catalogue information</rdfs:label>
    <dct:source>http://standards.iso.org/iso/19115/ed-1/en/</dct:source>
    <owl:versionInfo>2003-05-0</owl:versionInfo>
    <owl:imports rdf:resource="http://def.isotc211.org/iso19115/2003/CitationAndResponsiblePartyInformation"/>
    <owl:imports rdf:resource="http://def.isotc211.org/iso19150/-2/2012/base"/>
  </owl:Ontology>
  <owl:Class rdf:about="&iso19115;MD_PortrayalCatalogueReference">
    <rdfs:label>MD_PortrayalCatalogueReference</rdfs:label>
    <skos:definition>Information identifing the portrayal catalogue used</skos:definition>
    <rdfs:isDefinedBy>http://standards.iso.org/iso/19115/ed-1/en/</rdfs:isDefinedBy>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19115;MD_PortrayalCatalogueReference.portrayalCatalogueCitation"/>
        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19115;MD_PortrayalCatalogueReference.portrayalCatalogueCitation"/>
        <owl:allValuesFrom rdf:resource="&iso19115_613;CI_Citation"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:ObjectProperty rdf:about="&iso19115;MD_PortrayalCatalogueReference.portrayalCatalogueCitation">
    <rdfs:label>portrayalCatalogueCitation</rdfs:label>
    <rdfs:isDefinedBy>http://standards.iso.org/iso/19115/ed-1/en/</rdfs:isDefinedBy>
    <rdfs:domain rdf:resource="&iso19115;MD_PortrayalCatalogueReference"/>
    <rdfs:range rdf:resource="&iso19115_613;CI_Citation"/>
  </owl:ObjectProperty>
</rdf:RDF>

