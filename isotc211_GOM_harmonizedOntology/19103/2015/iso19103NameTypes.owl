    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19103;GenericName.scope"/>
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="&iso19103;GenericName.scope"/>
        <owl:allValuesFrom rdf:resource="&iso19103;NameSpace"/>
