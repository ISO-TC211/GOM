PREFIX : <http://def.isotc211.org/iso19160/-1/2015/Address/code/AddressStatus/>
PREFIX cs: <http://def.isotc211.org/iso19160/-1/2015/Address/code/AddressStatus>
PREFIX dcterms: <http://purl.org/dc/terms/>
PREFIX prov: <http://www.w3.org/ns/prov#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX reg: <http://purl.org/linked-data/registry#>
PREFIX schema: <https://schema.org/>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX status: <http://def.isotc211.org/iso19135/-1/2015/CoreModel/code/RE_ItemStatus/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

:official
    a skos:Concept ;
    dcterms:identifier "official"^^xsd:token ;
    dcterms:provenance "Presented in the original standard's codelist"@en ;
    reg:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "An official addressing authority assigned the address." ;
    skos:inScheme cs: ;
    skos:prefLabel "official"@en ;
    skos:topConceptOf cs: ;
.

:unknown
    a skos:Concept ;
    dcterms:identifier "unknown"^^xsd:token ;
    dcterms:provenance "Presented in the original standard's codelist"@en ;
    reg:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The status of the address is unknown." ;
    skos:inScheme cs: ;
    skos:prefLabel "unknown"@en ;
    skos:topConceptOf cs: ;
.

:unofficial
    a skos:Concept ;
    dcterms:identifier "unofficial"^^xsd:token ;
    dcterms:provenance "Presented in the original standard's codelist"@en ;
    reg:status status:original ;
    rdfs:isDefinedBy cs: ;
    skos:definition "The address was not assigned by an official addressing authority." ;
    skos:inScheme cs: ;
    skos:prefLabel "unofficial"@en ;
    skos:topConceptOf cs: ;
.

cs:
    a skos:ConceptScheme ;
    dcterms:created "2000-01-01"^^xsd:date ;
    dcterms:creator <http://def.isotc211.org/org/tc211> ;
    dcterms:identifier "AddressStatus"^^xsd:token ;
    dcterms:isFormatOf <http://def.isotc211.org/iso19160/-1/2015/Address#AddressStatus> ;
    dcterms:issued "2000-01-01"^^xsd:date ;
    dcterms:modified "2000-01-01"^^xsd:date ;
    dcterms:provenance "Derived from TC211-produced RDF versions of the standard's UML"@en ;
    dcterms:publisher <http://def.isotc211.org/org/tc211> ;
    dcterms:source "http://standards.iso.org/iso/19160/-1/ed-1/en/"^^xsd:anyURI ;
    skos:definition "An ISO standard's ConceptScheme"@en ;
    skos:hasTopConcept
        :official ,
        :unknown ,
        :unofficial ;
    skos:prefLabel "Address Status"@en ;
    prov:qualifiedDerivation [
            prov:hadRole <http://linked.data.gov.au/def/vocab-derivation-modes/none>
        ] ;
.

